{
  "name": "projet",
  "version": "0.1.0",
  "description": "Proje Bot Discord",
  "main": "main.js",
  "repository": "https://github.com/Hookcas/discordjsbot.git",
  "author": "Alexis <alexis.wenner2@orange.fr>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-aqua": "^6.0.1"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "prefer-template": "off",
      "no-eval": "off"
    }
  }
}